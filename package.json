{
  "name": "three-actor",
  "version": "0.0.3",
  "description": "Character framework for three.js",
  "scripts": {
    "start": "npm run clean && parcel test/TestActor.html",
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist ./.cache ./lib",
    "deploy": "npm run build && gh-pages -d dist",
    "test": "jest",
    "pub": "git push --tags && npm publish",
    "gitadd": "git add --all -- :!lib/*"
  },
  "keywords": [
    "TypeScript",
    "ThreeJs",
    "Three.js"
  ],
  "author": "Paul Elliott",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@types/node": "^13.5.3",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "gh-pages": "^2.2.0",
    "jest": "^25.1.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.1.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "three": "*"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "three": "^0.114.0"
  }
}
